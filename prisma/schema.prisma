generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type PeopleName {
  first String
  last  String
}

model people {
  id       String     @id @default(auto()) @map("_id") @db.ObjectId
  contribs String[]
  name     PeopleName
  views    BigInt
}
